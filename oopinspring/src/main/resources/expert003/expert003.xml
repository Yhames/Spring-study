<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="koreanTire" class="study.oopinspring.expert003.KoreanTire"></bean>
    <bean id="americanTire" class="study.oopinspring.expert003.AmericanTire"></bean>
    <bean id="car" class="study.oopinspring.expert003.Car">
        <property name="tire" ref="koreanTire"></property>
    </bean>

</beans>